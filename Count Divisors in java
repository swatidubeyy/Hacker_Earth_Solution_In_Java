import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner scn = new Scanner(System.in);
        int l = scn.nextInt();
        int r = scn.nextInt();
        int k = scn.nextInt();

        int count = 0;
        for(int i = l; i <= r ; i++){
            if(i % k == 0){
                count++;
            }
        }
        System.out.println(count);

    }
}
